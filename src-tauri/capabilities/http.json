{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "http-requests",
  "description": "allows HTTP requests to GitHub API",
  "windows": ["main"],
  "permissions": [
    "http:default",
    {
      "identifier": "http:allow-fetch",
      "allow": [
        {
          "url": "https://api.github.com/**"
        }
      ]
    }
  ]
}